{
    "name": "jstormes/db-tool",
    "description": "Tool to build databases and set permissions",
    "type": "library",
    "license": "GPL 3",
    "authors": [
        {
            "name": "James Stormes",
            "email": "jstormes@stormes.net"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-pdo": "*",
        "symfony/console": "^4.3",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-config-aggregator": "^1.0",
        "psr/log": "^1.1",
        "zendframework/zend-log": "^2.11"
    },
    "autoload": {
        "psr-4": {
